{
  "name": "boggle",
  "version": "0.1.4",
  "description": "Boggle grid solver",
  "main": "index.js",
  "preferGlobal": "true",
  "bin": {
    "boggle": "./index.js"
  },
  "scripts": {
    "test": "gt src/test/*.js --output",
    "complexity": "jsc index.js src/*.js",
    "commit": "./node_modules/pre-git/node_modules/commitizen/bin/git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bahmutov/boggle.git"
  },
  "private": false,
  "keywords": [
    "boggle",
    "solver",
    "words"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "check-types": "1.4.0",
    "lodash": "3.10.1",
    "matrix-paths-zolmeister": "0.0.4",
    "prefix-dictionary": "0.0.2"
  },
  "devDependencies": {
    "gt": "0.8.49",
    "jsc": "0.3.11",
    "pre-git": "1.2.1"
  },
  "config": {
    "pre-git": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": [
        "npm test",
        "npm version"
      ],
      "pre-push": [
        "rm -rf node_modules",
        "npm install",
        "npm test"
      ],
      "post-commit": [],
      "post-merge": []
    }
  },
  "czConfig": {
    "path": "node_modules/pre-git/node_modules/cz-conventional-changelog"
  }
}
